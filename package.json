    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.13",
    "@mui/material": "^5.16.13",
    "@types/react-dropzone": "^4.2.2",
    "cors": "^2.8.5",
    "electron": "^28.1.0",
    "express": "^4.21.2",
    "i18next": "^23.7.16",
    "node-machine-id": "^1.1.12",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-i18next": "^14.0.0",
