{
  "name": "WiFly",
  "version": "1.0.0",
  "description": "局域网文件传输工具",
  "main": "dist/main.js",
  "scripts": {
    "start": "wait-on dist/main.js && electron . --inspect --trace-warnings",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:watch\" \"npm:start\"",
    "watch": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js",
    "package": "electron-builder",
    "test": "jest",
    "server": "ts-node server/index.ts"
  },
